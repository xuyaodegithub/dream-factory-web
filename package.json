{
  "name": "create",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "serve": "vite --port 8080",
    "build_test": "run-p type-check \"build-only-test {@}\" --",
    "build_prod": "run-p type-check \"build-only-prod {@}\" --",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only-test": "vite build --mode test",
    "build-only-prod": "vite build --mode prod",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@ant-design/icons-vue": "^7.0.1",
    "@authing/guard-vue3": "^5.3.2",
    "@authing/web": "^5.1.9",
    "ant-design-vue": "4.0.6",
    "axios": "^1.6.2",
    "less": "^4.2.0",
    "less-loader": "^11.1.3",
    "pinia": "^2.1.7",
    "unplugin-vue-components": "^0.26.0",
    "vue": "3.3.10",
    "vue-router": "4.2.5"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.3",
    "@tsconfig/node18": "^18.2.2",
    "@types/node": "^18.19.2",
    "@vitejs/plugin-vue": "^4.5.1",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.4.0",
    "eslint": "8.22.0",
    "eslint-plugin-vue": "^9.17.0",
    "npm-run-all2": "^6.1.1",
    "prettier": "^3.0.3",
    "typescript": "~5.2.0",
    "vite": "^5.0.5",
    "vue-tsc": "^1.8.25"
  }
}
